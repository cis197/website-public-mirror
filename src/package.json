{
  "name": "website",
  "private": true,
  "description": "Website for CIS 197: JavaScript Web Development",
  "version": "0.1.1",
  "author": "CIS 197 Staff <ccabo@seas.upenn.edu>",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "removeStatic": "rm -rf public .cache",
    "dev": "yarn develop",
    "rebuild": "yarn removeStatic && yarn develop",
    "start": "npm run develop",
    "format": "prettier --write \"./src/**/*.{js,jsx}\"",
    "lint": "eslint \"./src/**/*.{js,jsx}\"",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format && yarn lint --fix"
    }
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.5.5",
    "@mdx-js/react": "^1.5.5",
    "babel-plugin-styled-components": "^1.10.0",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "gatsby": "^2.15.19",
    "gatsby-image": "^2.2.20",
    "gatsby-plugin-manifest": "^2.2.18",
    "gatsby-plugin-mdx": "^2.14.1",
    "gatsby-plugin-offline": "^3.0.8",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-sharp": "^2.2.25",
    "gatsby-plugin-styled-components": "^3.1.6",
    "gatsby-remark-copy-linked-files": "^2.1.20",
    "gatsby-remark-prismjs": "^3.3.14",
    "gatsby-source-filesystem": "^2.1.25",
    "gatsby-transformer-json": "^2.2.26",
    "gatsby-transformer-remark": "^2.6.24",
    "gatsby-transformer-sharp": "^2.2.16",
    "husky": "^4.2.3",
    "prism-react-renderer": "^1.0.2",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {},
  "keywords": [
    "cis",
    "197",
    "cis197",
    "penn",
    "upenn",
    "university",
    "pennsylvania",
    "js",
    "javascript",
    "react",
    "node",
    "express",
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cis197/website"
  },
  "bugs": {
    "url": "https://github.com/cis197/website/issues"
  }
}
